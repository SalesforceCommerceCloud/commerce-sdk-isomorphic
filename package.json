    "nock": "^13.2.8",
    "node-fetch": "2.6.13",
    "npm-pack-all": "^1.12.7",
    "postcss-preset-env": "6.7.1",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
