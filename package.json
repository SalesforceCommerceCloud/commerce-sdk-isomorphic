{
  "name": "commerce-sdk-react",
  "version": "0.1.0",
  "description": "Salesforce Commerce SDK for React",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalesforceCommerceCloud/commerce-sdk-react.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/SalesforceCommerceCloud/commerce-sdk-react/issues"
  },
  "private": false,
  "dependencies": {
    "fs-extra": "^9.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "build:lib": "npx rollup -c",
    "fix:js": "npx eslint --fix ./src/",
    "lint:js": "npx eslint ./src/",
    "lint:style": "npx stylelint ./src/",
    "fix:style": "npx stylelint ./src/ --fix",
    "start:docs": "docz dev",
    "build:docs": "docz build",
    "renderTemplates": "ts-node --compiler-options '{\"esModuleInterop\":true, \"target\": \"ES6\"}' generate.ts",
    "tsc": "tsc --build generate.tsconfig.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "style": "lib/default.css",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "CI=true yarn test --passWithNoTests"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.1",
    "@commerce-apps/raml-toolkit": "^0.5.0",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@testing-library/jest-dom": "5.11.2",
    "@testing-library/react": "10.4.7",
    "@testing-library/react-hooks": "3.4.1",
    "@testing-library/user-event": "12.0.17",
    "autoprefixer": "9.8.5",
    "docz": "2.3.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "handlebars-helpers": "^0.10.0",
    "husky": "4.2.5",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "lint-staged": "10.2.11",
    "npm-run-all": "4.1.5",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "react-scripts": "3.4.3",
    "react-test-renderer": "16.13.1",
    "recursive-copy": "2.0.10",
    "rimraf": "3.0.2",
    "rollup": "2.22.2",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-filesize": "9.0.2",
    "rollup-plugin-includepaths": "0.2.3",
    "rollup-plugin-peer-deps-external": "2.2.3",
    "rollup-plugin-postcss": "3.1.3",
    "rollup-plugin-stylelint": "1.0.0",
    "rollup-plugin-terser": "6.1.0",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "proxy": "https://SHORT_CODE.api.commercecloud.salesforce.com"
}
