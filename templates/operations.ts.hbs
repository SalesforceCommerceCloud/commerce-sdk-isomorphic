        "{{{../path}}}",
        this.clientConfig.baseUri,
        {
          pathParams,
          queryParams,
          origin: this.clientConfig.proxy
        }
      );

      const headers: Record<string, string> = {
        {{#if (isRequestWithPayload request)}}
        "Content-Type": "{{{getMediaTypeFromRequest request}}}",
        {{/if}}
        ...this.clientConfig.headers,
        ...options?.headers
